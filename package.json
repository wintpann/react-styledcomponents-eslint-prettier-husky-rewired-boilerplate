{
  "name": "react-styledcomponents-eslint-prettier-husky-rewired-boilerplate",
  "version": "0.0.0",
  "private": false,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "homepage": ".",
  "scripts": {
    "dev": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "lint:tsx": "lint-staged",
    "lint:styled": "stylelint './src/**/*.styled.ts' --allow-empty-input",
    "lint:css": "prettier --write \"./src/**/*.{styled.ts,css}\" --no-error-on-unmatched-pattern",
    "lint": "npm run lint:tsx && npm run lint:styled && npm run lint:css",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.0",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^5.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.1.2"
  }
}
